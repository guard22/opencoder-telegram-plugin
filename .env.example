# Telegram Bot Token (from @BotFather)
BOT_TOKEN=your_bot_token_here

# Cloudflare KV namespace ID (created via wrangler kv namespace create "USERS")
KV_NAMESPACE_ID=your_kv_namespace_id

# Cloudflare Account ID (found in Cloudflare dashboard)
CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id

# Worker URL (available after first deployment)
WORKER_URL=https://opencode-telegram-bot.your-subdomain.workers.dev
